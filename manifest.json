{
  "manifest_version": 3,
  "name": "alt checker",
  "version": "1.0",
  "description": "開いているページのimg要素を抽出し、alt属性の確認が可能。画像サムネイルとCSVダウンロード機能も提供します。",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "web_accessible_resources": [{
    "resources": ["result.html", "i18n.js", "result.js"],
    "matches": ["<all_urls>"]
  }],
  "action": {
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
  ,
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}